<h1>Listing fav_line_items</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Fav</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fav_line_items.each do |fav_line_item| %>
  <tr>
    <td><%= fav_line_item.product_id %></td>
    <td><%= fav_line_item.fav_id %></td>
    <td><%= link_to 'Show', fav_line_item %></td>
    <td><%= link_to 'Edit', edit_fav_line_item_path(fav_line_item) %></td>
    <td><%= link_to 'Destroy', fav_line_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

